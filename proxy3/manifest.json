{
  "manifest_version": 3,
  "version": "1.0.1",
  "name": "Chrome Proxy",
  "permissions": [
    "proxy",
    "tabs",
    "storage",
    "webRequest",
    "webRequestAuthProvider",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
